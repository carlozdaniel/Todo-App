<div class="large-container center-block">
  <h1> <%= @article.title%></h1>
  <div>
    <p> Escrito por: <strong><%= @article.user.email%></strong></p>
    <% if current_user.avatar.attached?%>
    <%= image_tag url_for(current_user.avatar), height:30 %>
    <% end%>
  </div>
  <div>
    <%= @article.content%>
  </div>
  <div>
    <%=link_to "elimina el articulo", @article, method: :delete %>
  </div>
</div>